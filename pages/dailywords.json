backgroundImg: "http://static.esdict.cn/MediaPool/daymonthimg/File411.jpg",
audio: "https://api.frdic.com/api/v2/speech/speakweb?langid=tingen&voicename=392f8340-f5a5-4f24-bc3c-7f148f0fdb89&txt=QYNRmxvd2VycyBkb24ndCB3b3JyeSBhYm91dCBob3cgdGhleSdyZSBnb2luZyB0byBibG9vbS4gVGhleSBqdXN0IG9wZW4gdXAgYW5kIHR1cm4gdG93YXJkIHRoZSBsaWdodCBhbmQgdGhhdCBtYWtlcyB0aGVtIGJlYXV0aWZ1bC4%3d",
},
{
sentence: {
